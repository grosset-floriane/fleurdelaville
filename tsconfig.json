{
    "compilerOptions": {
      "jsx": "preserve",
      "target": "es5",
      "baseUrl": "./src",
      "noEmit": true,
      "allowJs": true,
      "checkJs": false,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "allowSyntheticDefaultImports": true,
      "module": "esnext",
      "lib": [
        "es6",
        "dom"
      ],
      "strictNullChecks": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "incremental": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  